# Azure AI Search Configuration
search_url=https://YOUR-SEARCH-SERVICE.search.windows.net
search_api_key=YOUR_SEARCH_API_KEY_HERE

# Search Index Names (knowledge sources)
index_insurance=contoso-insurance-faq-index
index_retail=contoso-retail-index
index_gaming=contoso-gaming-index
index_financials=knowledgesource-1nykaa-financials

# Knowledge Base Name
knowledge_base_name=YOUR_KNOWLEDGE_BASE_NAME

# Azure OpenAI Configuration
az-openai_endpoint=https://YOUR-OPENAI-ENDPOINT.openai.azure.com/
az-openai-model=gpt-4o
az-openai-deployment=gpt-4o
az-openai-key=YOUR_AZURE_OPENAI_API_KEY_HERE
